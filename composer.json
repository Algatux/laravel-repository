{
    "name": "algatux/laravel-repository",
    "description": "Laravel 5 Model Repository",
    "keywords": ["Laravel", "Eloquent", "Models", "Repository", "Pattern"],
    "type": "library",
    "require": {
        "php": ">=5.5",
        "illuminate/support" : "~5.1",
        "illuminate/database" : "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "autoload": {
      "psr-4": {
        "Algatux\\Repository\\": "src/",
        "Algatux\\Repository\\Tests\\": "tests/"
      }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alessandro Galli",
            "email": "a.galli85@gmail.com",
            "role": "Developer"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
